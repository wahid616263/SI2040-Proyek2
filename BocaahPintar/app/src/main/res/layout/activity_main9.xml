<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tebakwarna"
    tools:context=".MainActivity8">

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="275dp"
        android:layout_height="219dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/apelmerah" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="213dp"
        android:layout_height="48dp"
        android:layout_marginBottom="44dp"
        android:autoText="true"
        android:text="Yah Salah :("
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="330dp"
        android:layout_height="55dp"
        android:layout_marginBottom="64dp"
        android:autoText="true"
        android:text="Ini adalah Tomat Merah"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/imageView33"
        app:layout_constraintEnd_toEndOf="parent"
